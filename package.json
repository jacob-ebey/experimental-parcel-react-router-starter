{
  "name": "parcel-react-server-starter",
  "private": true,
  "version": "0.0.0",
  "targets": {
    "server": {
      "source": "app/server.ts",
      "context": "react-server",
      "scopeHoist": false,
      "includeNodeModules": {
        "body-parser": false,
        "express": false
      }
    }
  },
  "scripts": {
    "build": "parcel build",
    "clean": "rm -rf dist node_modules .parcel-cache .react-router .react-router-parcel",
    "dev": "parcel",
    "start": "node ./dist/server/server.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.16",
    "@ai-sdk/react": "^1.2.9",
    "@parcel/runtime-rsc": "^2.14.4",
    "@types/body-parser": "^1.19.5",
    "ai": "^4.3.9",
    "body-parser": "^2.2.0",
    "express": "^4.21.2",
    "ollama-ai-provider": "^1.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "0.0.0-experimental-573acd4d4",
    "react-server-dom-parcel": "^19.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@react-router/dev": "0.0.0-experimental-573acd4d4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^4",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "buffer": "^5.5.0||^6.0.0",
    "parcel": "^2.14.4",
    "parcel-config-react-router-experimental": "^1.0.8",
    "prettier": "^3.5.3"
  }
}
