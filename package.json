{
  "name": "parcel-react-server-starter",
  "private": true,
  "version": "0.0.0",
  "server": "dist/server.js",
  "targets": {
    "server": {
      "source": "src/server.tsx",
      "context": "react-server",
      "scopeHoist": false,
      "includeNodeModules": {
        "@mjackson/node-fetch-server": false,
        "express": false
      }
    }
  },
  "scripts": {
    "build": "parcel build",
    "clean": "rm -rf dist node_modules",
    "dev": "parcel",
    "start": "node ./dist/server.js"
  },
  "dependencies": {
    "@mjackson/node-fetch-server": "^0.6.1",
    "@parcel/runtime-rsc": "^2.14.4",
    "express": "^4.21.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "0.0.0-experimental-0fd8a0272",
    "react-server-dom-parcel": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/plugin-syntax-decorators": "^7.25.9",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@parcel/config-default": "^2.14.4",
    "@parcel/plugin": "^2.14.4",
    "@react-router/dev": "0.0.0-experimental-36a5255b6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/express": "^4",
    "@types/node": "^22.10.5",
    "@types/parcel-env": "^0.0.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "babel-dead-code-elimination": "^1.0.10",
    "execa": "^9.5.2",
    "jiti": "^2.4.2",
    "oxc-parser": "^0.63.0",
    "oxc-transform": "^0.63.0",
    "parcel": "^2.14.4",
    "prettier": "^3.5.3"
  }
}
